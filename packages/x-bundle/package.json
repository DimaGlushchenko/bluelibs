{
  "name": "@bluelibs/x-bundle",
  "version": "1.0.0",
  "description": "X-Framework is a feature-rich solution with fast prototyping in mind for BlueLibs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluelibs/bluelibs"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "test": "jest --verbose dist/__tests__/index.js --passWithNoTests",
    "test:dev": "jest --verbose --watch src/__tests__/index.ts --detectOpenHandles",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "package:generate": "ts-node package.ts"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "date-fns": "^2.14.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "queue": "^6.0.2",
    "redis": "^3.0.2"
  },
  "peerDependencies": {
    "@bluelibs/core": "^1.0.0",
    "@bluelibs/ejson": "^1.0.0",
    "@bluelibs/graphql-bundle": "^1.0.0",
    "@bluelibs/logger-bundle": "^1.0.0",
    "@bluelibs/mongo-bundle": "^1.0.0",
    "@bluelibs/nova": "^1.0.0",
    "@bluelibs/security-bundle": "^1.0.0",
    "@bluelibs/validator-bundle": "^1.0.0",
    "graphql": "^15.3.0 || ^14.0.0"
  },
  "devDependencies": {
    "@bluelibs/apollo-bundle": "^1.0.0",
    "@bluelibs/core": "^1.0.0",
    "@bluelibs/ejson": "^1.0.0",
    "@bluelibs/email-bundle": "^1.0.0",
    "@bluelibs/graphql-bundle": "^1.0.0",
    "@bluelibs/logger-bundle": "^1.0.0",
    "@bluelibs/mongo-bundle": "^1.0.0",
    "@bluelibs/nova": "^1.0.0",
    "@bluelibs/security-bundle": "^1.0.0",
    "@bluelibs/validator-bundle": "^1.0.0",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^26.0.0",
    "@types/node": "^8.0.28",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "graphql": "^15.5.1",
    "jest": "^27.0.6",
    "jest-junit": "^10.0.0",
    "mongodb": "^3.6.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.13",
    "ts-jest": "^27.0.3",
    "typescript": "^4.0.2",
    "yup": "^0.32.9"
  },
  "types": "dist/index.d.ts",
  "license": "MIT"
}